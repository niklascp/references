number = {5},
pages = {1053--1061},
pmid = {17151871},
title = {{Molecular characterization of Bifidobacterium longum biovar longum NAL8 plasmids and construction of a novel replicon screening system}},
volume = {74},
year = {2007}
}
@article{Liu1998,
abstract = {Understanding of the scene content of a video sequence is very important{\$}\backslash{\$}nfor content-based indexing and retrieval of multimedia databases.{\$}\backslash{\$}nResearch in this area in the past several years has focused on the use{\$}\backslash{\$}nof speech recognition and image analysis techniques. As a complimentary{\$}\backslash{\$}neffort to the prior work, we have focused on using the associated audio{\$}\backslash{\$}ninformation (mainly the nonspeech portion) for video scene analysis. As{\$}\backslash{\$}nan example, we consider the problem of discriminating five types of TV{\$}\backslash{\$}nprograms, namely commercials, basketball games, football games, news{\$}\backslash{\$}nreports, and weather forecasts. A set of low-level audio features are{\$}\backslash{\$}nproposed for characterizing semantic contents of short audio clips. The{\$}\backslash{\$}nlinear separability of different classes under the proposed feature{\$}\backslash{\$}nspace is examined using a clustering analysis. The effective features{\$}\backslash{\$}nare identified by evaluating the intracluster and intercluster{\$}\backslash{\$}nscattering matrices of the feature space. Using these features, a neural{\$}\backslash{\$}nnet classifier was successful in separating the above five types of TV{\$}\backslash{\$}nprograms. By evaluating the changes between the feature vectors of{\$}\backslash{\$}nadjacent clips, we also can identify scene breaks in an audio sequence{\$}\backslash{\$}nquite accurately. These results demonstrate the capability of the{\$}\backslash{\$}nproposed audio features for characterizing the semantic content of an{\$}\backslash{\$}naudio sequence.},
author = {Liu, Z and Wang, Y and Chen, T H},
doi = {10.1023/A:1008066223044},
issn = {0922-5773},
journal = {J. Vlsi Signal Process. Syst. Signal Image Video Technol.},
number = {1-2},
pages = {61--79},
title = {{Audio feature extraction and analysis for scene segmentation and classification}},
volume = {20},
year = {1998}
}
