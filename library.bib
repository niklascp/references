isbn = {1932-6203},
issn = {19326203},
journal = {PLoS One},
number = {2},
pmid = {23469017},
title = {{Proteomic Analysis of Bifidobacterium longum subsp. infantis Reveals the Metabolic Insight on Consumption of Prebiotics and Host Glycans}},
volume = {8},
year = {2013}
}
