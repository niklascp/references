@book{RenewableFuelsAssociation2016,
abstract = {Amidst the seemingly endless drama attributable to the legislative or regulatory battle de jour and$\backslash$nthe ever increasing din of misleading advertising funded by the incumbent energy industry, one$\backslash$nmight be forgiven for losing sight of the fact that ethanol is still a business. So, while not ignoring$\backslash$nthe policy issues that will most certainly continue to shape the industry's trajectory in 2016 and$\backslash$nbeyond, this year's Outlook responds to a collective desire to return to the business of ethanol –$\backslash$none that has revitalized rural communities, created jobs in production, processing and marketing$\backslash$nacross the country, and provided consumer savings at the pump.$\backslash$nThe politics of ethanol may be rooted in its renewable, home-grown, and clean burning$\backslash$ncharacteristics. But the business of ethanol is rooted in octane. The world motor fuel market is$\backslash$nshort on octane, and ethanol is the lowest-cost source of octane available. Moreover, future growth$\backslash$nin the industry is quite likely tied directly to automaker efforts to meet increasingly stringent fuel$\backslash$neconomy standards with higher compression ratio engines that will require even higher octane fuels.$\backslash$nSo understanding the octane benefits of ethanol, particularly when compared to potential$\backslash$ncompetitors in the market, is critical to an appreciation for future challenges and opportunities$\backslash$nfacing the U.S. ethanol industry. These pages will provide an octane primer, along with a review of$\backslash$nethanol's place in the market today.$\backslash$nWhile 2015 was a far cry from 2014 in terms of profitability, last year was still another solid year$\backslash$nof growth in the face of falling oil prices and policy instability. Indeed, in November, the industry$\backslash$nhit the phenomenal production milestone of 1 million barrels per day! That's the equivalent of 15.3$\backslash$nbillion gallons on an annualized basis. The industry also saw DuPont open the world's largest$\backslash$ncellulosic ethanol production facility in Nevada, Iowa. Congratulations!$\backslash$nThe industry's commercial success only added to the frustration when EPA, on November 30,$\backslash$nfinalized an RFS rule that adopted the oil companies' narrative about the blend wall and reduced$\backslash$nthe 2016 RFS obligation for undifferentiated biofuels (corn ethanol) to 14.5 billion gallons, 800$\backslash$nmillion gallons less than our demonstrated production capability. Of course, the number was not$\backslash$nthe main issue. What EPA's rule really did was to fatally undermine the ability of the RFS's credit$\backslash$ntrading mechanism (RINs) to incentivize investment in the new technologies or infrastructure that$\backslash$nis necessary to fuel further market expansion. That is a drama that will most certainly continue to$\backslash$nimpact the legislative, regulatory, legal, and marketplace reality throughout 2016. Stay tuned.$\backslash$nIn the meanwhile, enjoy this year's Outlook, the U.S. ethanol industry's most trusted and cited$\backslash$nresource for the facts and trends driving the industry forward – this year in high octane fashion.},
author = {{Renewable Fuels Association}},
pages = {1--40},
title = {{Fueling a high octane future}},
url = {http://www.ethanolrfa.org/resources/publications/outlook/},
year = {2016}
}
@article{Wang2012,
abstract = {Globally, bioethanol is the largest volume biofuel used in the transportation sector, with corn-based ethanol production occurring mostly in the US and sugarcane-based ethanol production occurring mostly in Brazil. Advances in technology and the resulting improved productivity in corn and sugarcane farming and ethanol conversion, together with biofuel policies, have contributed to the significant expansion of ethanol production in the past 20 years. These improvements have increased the energy and greenhouse gas (GHG) benefits of using bioethanol as opposed to using petroleum gasoline. This article presents results from our most recently updated simulations of energy use and GHG emissions that result from using bioethanol made from several feedstocks. The results were generated with the GREET (Greenhouse gases, Regulated Emissions, and Energy use in Transportation) model. In particular, based on a consistent and systematic model platform, we estimate life-cycle energy consumption and GHG emissions from using ethanol produced from five feedstocks: corn, sugarcane, corn stover, switchgrass and miscanthus. We quantitatively address the impacts of a few critical factors that affect life-cycle GHG emissions from bioethanol. Even when the highly debated land use change GHG emissions are included, changing from corn to sugarcane and then to cellulosic biomass helps to significantly increase the reductions in energy use and GHG emissions from using bioethanol. Relative to petroleum gasoline, ethanol from corn, sugarcane, corn stover, switchgrass and miscanthus can reduce life-cycle GHG emissions by 19–48{\%}, 40–62{\%}, 90–103{\%}, 77–97{\%} and 101–115{\%}, respectively. Similar trends have been found with regard to fossil energy benefits for the five bioethanol pathways.},
author = {Wang, Michael and Han, Jeongwoo and Dunn, Jennifer B and Cai, Hao and Elgowainy, Amgad},
doi = {10.1088/1748-9326/7/4/045905},
isbn = {1748-9326},
issn = {1748-9326},
journal = {Environ. Res. Lett.},
keywords = {biofuels,cellulosic ethanol,corn ethanol,energy balance,greenhouse gas emissions,life-cycle analysis,sugarcane ethanol},
number = {4},
pages = {13},
title = {{Well-to-wheels energy use and greenhouse gas emissions of ethanol from corn, sugarcane and cellulosic biomass for US use}},
url = {http://stacks.iop.org/1748-9326/7/i=4/a=045905?key=crossref.bc2d92022ddca565108ad62fb6e4201d},
volume = {7},
year = {2012}
}
