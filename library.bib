journal = {Int. J. Numer. Methods Eng.},
keywords = {dislocation,molecular dynamics,multiscale simulation,nanomechanics,non-equilibrium},
month = {jul},
number = {May},
pages = {67--97},
pmid = {260949200001},
title = {{Explicit level-set-based topology optimization using an exact Heaviside function and consistent sensitivity analysis}},
url = {http://doi.wiley.com/10.1002/nme.4258 http://journals.cambridge.org/abstract{\_}S1551929513000503},
