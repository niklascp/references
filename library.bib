isbn = {0378-1097 (Print)},
issn = {03781097},
journal = {FEMS Microbiol. Lett.},
keywords = {Broad host range,Lactococcus lactis,Ovine interferon-omega,Secretion,Staphylococcal nuclease},
number = {2},
pages = {307--313},
pmid = {12892897},
title = {{Controlled intra- or extracellular production of staphylococcal nuclease and ovine omega interferon in Lactococcus lactis}},
url = {http://journals.cambridge.org/abstract{\_}S1551929513000503},
volume = {224},
year = {2003}
}
