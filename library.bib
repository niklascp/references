@phdthesis{Nielsen2014,
abstract = {The Brazilian sugar cane industry constitute one of the biggest and most efficient ethanol production platforms in the world. Brazilian ethanol production utilizes a unique process, which include cell recycling, acid wash and aseptic conditions. Features such as extensive CO2 generation, crude composition of the substrate and frequent contaminations, leads to excessive foam formation during the fermentations that is treated with chemical antifoaming agents (AFAs). This study has investigated the impact of AFA treatments on the physiology and transcriptome of the industrial ethanol Saccharomyces cerevisiae strain CAT-1. The investigated AFAs included (i) commercially available AFAs frequently cited in the fermentation literature, (ii) industrially used AFAs aquired from Brazilian ethanol plants (iii) and a novel AFA named Novozym{\textregistered} 50207. AFA treatments were investigated in defined laboratory batch cultivations and in laboratory simulation of the Brazilian ethanol production process. It was shown that in concentrations relevant for defoaming, industrially used AFAs reduced mmax and ethanol productivity while commercial AFAs showed no effect on cell physiology in terms of yields or productivity. When introduced to the industrial process, industrial defoaming treatments reduced cell viability compared to the control, and the effects where increasing with increasing AFA concentration and exposure time. Treatments with Novozym{\textregistered} 50207, on the other hand did not affect the yeast physiology as compared to the control. Transcriptome analysis showed that all AFA treatments induced additional stress responses in the yeast compared to the control, shown by an up-regulation of stress specific genes and/or a down-regulation of growth related processes. This study documents for the first time the detrimental effects associated with industrially used chemical defoaming treatments, and show the potential of developing novel systems for foam control to improve the production performance of industrial fermentation processes.},
author = {Nielsen, Jens Christian Fr{\o}slev},
number = {May},
pages = {74},
school = {Danish Technical University},
title = {{INFLUENCE OF ANTI FOAMING AGENTS ON THE CELLULAR PERFORMANCE OF SACCHAROMYCES CEREVISIAE IN THE BRAZILIAN BIOETHANOL INDUSTRY}},
year = {2014}
}
