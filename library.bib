@article{AlejandroYanez2004,
abstract = {Foram determinadas medidas biom{\'{e}}tricas de caprinos leiteiros com o objetivo de estimar equa{\c{c}}{\~{o}}es que permitam predizer o peso e as caracter{\'{i}}sticas da carca{\c{c}}a de animais sob diferentes condi{\c{c}}{\~{o}}es nutricionais. Realizou-se um experimento em duas fases, utilizando-se 27 cabritos machos da ra{\c{c}}a Saanen em cada uma, distribu{\'{i}}dos nos tratamentos: alimenta{\c{c}}{\~{a}}o {\`{a}} vontade, 30 e 60{\%} de restri{\c{c}}{\~{a}}o. O peso vivo (PV) inicial foi de 5 kg na Fase 1 e 20 kg na Fase 2, abatendo-se os animais ao atingirem 20 e 35 kg de PV, respectivamente. Foram determinadas as medidas biom{\'{e}}tricas e avaliada a condi{\c{c}}{\~{a}}o corporal no animal vivo e, ap{\'{o}}s o abate e resfriamento de 24 horas, as medidas da carca{\c{c}}a. Estimaram-se o peso em jejum (PJ), o peso da carca{\c{c}}a fria (PCF) e a compacidade da carca{\c{c}}a, em fun{\c{c}}{\~{a}}o das medidas biom{\'{e}}tricas. O per{\'{i}}metro tor{\'{a}}cico e o comprimento corporal foram as medidas biom{\'{e}}tricas que apresentaram melhor ajuste para estimar o PJ, o PCF e a compacidade da carca{\c{c}}a e, por isso, s{\~{a}}o recomendados, por serem precisos, pr{\'{a}}ticos e de f{\'{a}}cil execu{\c{c}}{\~{a}}o.},
author = {{Alejandro Y{\'{a}}{\~{n}}ez}, Enrique and de Resende, Kleber Tom{\'{a}}s and Ferreira, {\^{A}}ngela Cristina Dias and Medeiros, Ariosvaldo Nunes and da {Silva Sobrinho}, Am{\'{e}}rico Garcia and {Pereira Filho}, Jos{\'{e}} Morais and Teixeira, Izabelle Auxiliadora Molina de Almeida and Artoni, Silvana Martinez Baraldi},
