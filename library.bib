journal = {Gut},
keywords = {Administration,Animal,Animal: microbiology,Animals,Bacterial Translocation,Bacterial Translocation: drug effects,Bacterial Translocation: physiology,Cellulose,Cellulose: administration {\&} dosage,Dietary Carbohydrates,Dietary Carbohydrates: administration {\&} dosage,Dietary Fiber,Dietary Fiber: administration {\&} dosage,Enteritis,Enteritis: microbiology,Feces,Feces: microbiology,Intestinal Diseases,Intestinal Diseases: microbiology,Intestinal Mucosa,Intestinal Mucosa: microbiology,Intestines,Intestines: enzymology,Intestines: microbiology,Lactulose,Lactulose: administration {\&} dosage,Male,Mucins,Mucins: metabolism,Oligosaccharides,Oligosaccharides: administration {\&} dosage,Oral,Peroxidase,Peroxidase: metabolism,Rats,Salmonella Infections,Salmonella enteritidis,Salmonella enteritidis: physiology,Specific Pathogen-Free Organisms,Starch,Starch: administration {\&} dosage,Triticum,Wistar},
