@article{ThinkingOutsideTheBus2007,
abstract = {Efficiently and effectively run public transit systems are essential components of and crucial contributors to the proper functioning of a region's overall transportation network. However, travel by public transit is a complex process that involves more than simply people moving about on buses and trains. A typical door-to-door trip involves walking from one's origin to a bus stop or train station, waiting for a vehicle to arrive, boarding the vehicle, traveling in the vehicle, alighting from the vehicle, and then walking to one's final destination. In many cases, the trip also involves transfers; travelers alight from one transit vehicle, move to a new stop or platform, wait for another transit vehicle, board that vehicle and continue this process until they reach their last stop or station at which time they then must walk to their final destination. Therefore, transit travelers expend a great deal of their time, energy, and patience outside of buses and trains, and this contributes greatly to both their actual and perceived burden of transit travel. This added burden influences the travelers' decisions on whether or not to take transit in the future. Given the relative convenience of door-to-door travel by foot, bicycle, taxi, or private vehicle for many trips, public transit systems are often at a competitive disadvantage when competing for passengers. This research examines ways to increase the attractiveness of public transportation and reduce the perceived burden of the time spent outside of vehicles during transit trips.},
author = {{Taylor, Brian D; Iseki, Hiroyuki; Miller, Mark A; Smart}, Michael},
doi = {10.1111/jsap.12101},
file = {:C$\backslash$:/Users/ncp/Dropbox/Movia/Academic/References//2007 - Taylor, Brian D Iseki, Hiroyuki Miller, Mark A Smart.pdf:pdf},
isbn = {9780471690061},
issn = {1748-5827},
journal = {Calif. Deoartment Transp.},
number = {January},
pages = {42},
pmid = {23790051},
title = {{Thinking Outside the Bus: Understanding User Perceptions of Waiting and Transferring in Order to Increase Transit Use}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24169929},
year = {2007}
}
@article{Liu2007,
abstract = {Reliability of public transport systems has been considered critically important by public$\backslash$r$\backslash$ntransport users, operators and the government. Lack of reliability in public transport results in$\backslash$r$\backslash$nuncertainty and delays aggravating anxiety and discomfort for the passengers and increases$\backslash$r$\backslash$ncosts due to lost mileage and lower fleet utilisation for the operators. The government$\backslash$r$\backslash$nemphasises a reliable bus-service in order to ensure its competitiveness to car use such that$\backslash$r$\backslash$nit results in modal shift. Though reliability is of significance to a range of actors such as the$\backslash$r$\backslash$nbus operators and the passengers, its interpretations vary across different groups. For$\backslash$r$\backslash$nexample, quality of service identified by the demand-oriented indicators, the travellers, is$\backslash$r$\backslash$nsystematically different from what the supply-based quality indicators show. Reliability, as a$\backslash$r$\backslash$nmeasure of quality of service, is related to the operational characteristics of the public$\backslash$r$\backslash$ntransport system. Passenger's wait time, on the other hand, is much more sensitive to$\backslash$r$\backslash$nschedule reliability than to service frequency.$\backslash$r$\backslash$nThis paper investigates some of the measures used to assess reliability of an urban bus$\backslash$r$\backslash$nnetwork using a dynamic microsimulation model framework. The framework incorporates$\backslash$r$\backslash$nmodels of bus operation, passenger arrival and boarding for buses within a network model of$\backslash$r$\backslash$ngeneral traffic. It therefore allows explicit modelling of the interactions among cars, buses$\backslash$r$\backslash$nand passengers and of their effect on bus reliability. The model framework is applied to a$\backslash$r$\backslash$ncase study based on a bus route in the city of York. Empirical analysis of the test route was$\backslash$r$\backslash$ncarried out to understand the background of reliability issues and to establish a sound base$\backslash$r$\backslash$nscenario of the network. The simulation model is used to help understand the causes and$\backslash$r$\backslash$nprocess of occurrence of unreliability and to study the impacts of possible scenarios such as$\backslash$r$\backslash$nincreased congestion, rise in passenger demand, and reduction in ‘per passenger boarding$\backslash$r$\backslash$ntime' on the reliability of the test-route. The study measures reliability in terms of travel time,$\backslash$r$\backslash$nheadways and excess passenger wait time, and sheds light on factors affecting them.$\backslash$r$\backslash$nThe results of the study reveal that excess passenger wait time is strongly correlated with the$\backslash$r$\backslash$nvariation of headways and an increase in passenger demand intensifies the ‘bunching and$\backslash$r$\backslash$nspreading out' of buses. With increasing congestion and passenger demand, reliability is$\backslash$r$\backslash$nreduced. Reduction in per passenger boarding time proves to be an effective measure in$\backslash$r$\backslash$nimproving reliability.$\backslash$r$\backslash$nBeyond these specific case study results, the modelling framework developed may serve as$\backslash$r$\backslash$na useful tool to assess reliability levels for a future year and to evaluate proposed strategies$\backslash$r$\backslash$nto improve reliability before implementation. Such a modelling framework, if extended to$\backslash$r$\backslash$ncover larger bus networks can serve as an important decision-making tool in the hands of the$\backslash$r$\backslash$ngovernment and bus operators for offering more reliable bus services to the public.},
author = {Liu, Ronghui and Sinha, Shalini},
file = {:C$\backslash$:/Users/ncp/Dropbox/Movia/Academic/References/2007 - Liu, Sinha.pdf:pdf},
journal = {Third Int. Symp. Transp. Netw. Reliab.},
pages = {19--20},
title = {{Modelling urban bus service and passenger reliability}},
url = {http://eprints.whiterose.ac.uk/3686/},
year = {2007}
}
